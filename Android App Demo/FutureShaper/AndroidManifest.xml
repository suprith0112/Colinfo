<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.futureshaper"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name=".UniversityCh"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.futureshaper.UNIVERSITYCH"/>
                <category android:name="android.intent.category.DEFAULT"/>
                </intent-filter>
        </activity>
        <activity 
            android:name=".GovtExams"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.futureshaper.GOVTEXAMS"/>
                <category android:name="android.intent.category.DEFAULT"/>
                </intent-filter>
        </activity>
        <activity 
            android:name=".CetDetails"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.futureshaper.CETDETAILS"/>
                <category android:name="android.intent.category.DEFAULT"/>
                </intent-filter>
        </activity>
    </application>

</manifest>
